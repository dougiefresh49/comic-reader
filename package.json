{
  "name": "comic-reader",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "audio-downloader": "node scripts/audio-downloader.js",
    "video-download": "node scripts/video-download.js",
    "download-comic-images": "python3 scripts/download_comic_images.py",
    "get-context": "tsx --env-file=.env scripts/get-context.ts",
    "test-roboflow": "tsx --env-file=.env scripts/test-roboflow-request.ts"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "next": "^15.2.3",
    "p-limit": "^7.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.5",
    "youtube-dl-exec": "^3.0.26",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.15",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.40.0"
  },
  "packageManager": "pnpm@10.7.1"
}
